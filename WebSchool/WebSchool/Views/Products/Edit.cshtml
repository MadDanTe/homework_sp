@model IEnumerable<WebSchool.Models.Product>

@{
    ViewData["Title"] = "Edit";
}

<div class="row">
    <div class="col-md-3">
        <h2>Name:</h2>
        <ul>
            @foreach (var p in Model)
            {
                <li>@p.Name</li>
            }
        </ul>
    </div>
    <div class="col-md-3">
        <h2>Age:</h2>
        <ul>
            @foreach (var p in Model)
            {
                <li>@p.Price</li>
            }
        </ul>
    </div>

</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
